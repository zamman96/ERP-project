/* selectFrcsMenuList.css */
.menu-table tbody {
    display: block; /* 가로세로 조절 가능 */
    max-height: 461px; /* 보여주고 싶은 만큼 세로값 입력 */
    overflow-y: scroll; /* 스크롤 만듦 */
}

.menu-table thead, .menu-table tbody tr {
    display: table;
    width: 100%;
}

.menu-table thead th{
	padding: 0;	
}
input[type=checkbox], input[type=radio]{
    width: 20px;
}