}

/* width 값 모음 */
.w-100 {
	width: 100px !important;
}
.w-150 {
	width: 150px !important;
}
.w-200 {
	width: 200px !important;
}
.w-250 {
	width: 250px !important;
}
.w-300 {
	width: 300px !important;
}
.w-350 {
	width: 350px !important;
}
.w-400 {
	width: 400px !important;
}
.w-fill {
    width: -webkit-fill-available;
}

/* 툴팁 스타일 */
.tooltip-main { /* 툴팁 버튼 스타일 조정 */
  width: 400px;
  height: 15px;
  font-weight: 700;
  color: #737373;
  margin: 4px 121px 0 5px;
  float: right;
  text-align: left !important;
}
.tooltip-qm {
  float: right;
  margin: -2px 0px 3px 4px;
  font-size: 12px;
}
.tooltip-inner { /* 툴팁으로 나타나는 영역 스타일 조정 */
  max-width: 300px !important;
  font-size: 16px;
  padding: 10px 15px 10px 10px;
  background: var(--gray--0);
  color: rgba(0, 0, 0, .7);
  border: 1px solid rgba(43, 53, 71, 1);
  text-align: left;
}
.tooltip.show {
  opacity: 1;
}
.bs-tooltip-auto[x-placement^=bottom] .arrow::before,
.bs-tooltip-bottom .arrow::before {
  border-bottom-color: rgba(43, 53, 71, 1);
}
.tooltip-custom {
	padding: 0 !important;
	background: none !important;
}
.tooltip-icon {
	vertical-align: middle;
	color: rgba(43, 53, 71, 1);
	user-select: none;
}

/* 메뉴 */
.td-img img{
	width: 100%;
}

/* 필수 입력 항목 */
.es{
	color: var(--brand--accent);
}

/* 더보기 */
.moreView{
	text-align: center;
	cursor: pointer;
}

/* 차트 타이틀 */
.chart-title{
	display: flex;
}
#chart-title-text{
	position: relative;
	top: 5px;
}


/* tbody 스크롤바 */
tbody::-webkit-scrollbar {
    width: 7px;
    height: 7px;
}

tbody::-webkit-scrollbar-thumb {
    background: var(--gray--4);
    border-radius: 45px;
}

tbody::-webkit-scrollbar-thumb:hover {
    background: var(--green--4);
}

tbody::-webkit-scrollbar-track {
    background: transparent;
}

/* modal-body 스크롤바 */
.modal-body::-webkit-scrollbar {
    width: 7px;
    height: 7px;
}
.modal-body::-webkit-scrollbar-thumb {
    background: var(--gray--4);
    border-radius: 45px;
}
.modal-body::-webkit-scrollbar-thumb:hover {
    background: var(--green--4);
}
.modal-body::-webkit-scrollbar-track {
    background: transparent;
}

/* home 영역 시작 */
.home-wrap {
	margin-top: 20px;
}
.cont-title{
    display: flex;
    justify-content: space-between;
   	font-size: 1.3rem;	
}

/* btnBox 시작 */
.btnBox-wrap{
	width: -webkit-fill-available;
	display: inline-flex;
    gap: var(--gap--m);
    width: -webkit-fill-available;
    justify-content: space-between;
}
.btnBox-cont{
	box-shadow: 0px 8px 20px rgba(127, 138, 140, 0.09);
    background: linear-gradient(180deg, var(--yellow--2), var(--yellow--3));
    border-radius: 10px;
    overflow: hidden;
    padding: var(--pd--m);
    gap: var(--gap--m);
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    cursor: pointer;
    min-width: max-content;
    align-items: end;
}
.btnBox-cont:hover{
	background: linear-gradient(180deg, var(--yellow--3), var(--yellow--4));
}
.btnBox-dtl{
	display: flex;
	flex-direction: column;
	gap:var(--gap--s);
}
.btnBox-num{
	font-size: 1.5rem;
    font-weight: 900;
}
.btnBox-icon {
    display: flex;
    justify-content: center;
    align-items: center;
}
.btnBox-icon .icon-cnt {
	font-size: 30px;
	color: var(--gray--9);
}
.btnBox-title{
	font-size: 0.9rem;
	color: var(--gray--8);
}
/* btnBox 끝 */

/* 메인 게시판 영역 */
.board-box {
	border-radius: 0 10px 10px 10px !important;
}
.indicator li {
    /* box-shadow: 0px 8px 20px rgba(127, 138, 140, 0.09); */
    /* border-right: 3px solid transparent; */
    display: flex;
    padding: 15px;
    cursor: pointer;
    font-size: 1rem;
    color: var(--black);
    border-radius: var(--radius--m) var(--radius--m) 0 0;
    width: 150px;
    justify-content: center;
    background: var(--blue--8);
    color: white;
}
.indicator li i {
	font-size: 1rem;
}
.indicator li:hover {
	background: var(--blue--7);
}
.indicator li.active {
	border-top: 5px solid var(--blue--8);
    background: white;
    color: black;
    font-weight: bold;
    border-radius: 0;
    padding-top: 10px;
}





















