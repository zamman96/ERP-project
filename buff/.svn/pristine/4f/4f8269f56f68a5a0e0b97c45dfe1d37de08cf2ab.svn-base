@charset "UTF-8";

/* 나눔스퀘어 웹폰트 적용 */
@import url(https://cdn.jsdelivr.net/gh/moonspam/NanumSquare@2.0/nanumsquare.css); 
@import "reset.css";
@import "table.css";
@import "sort.css";
@import "layout.css";

:root {
	/* Color styles */
	--blue--0: rgba(255, 255, 255, 1);
	--blue--05: rgba(241, 246, 249, 1);
	--blue--1: rgba(237, 242, 247, 1);
	--blue--15: rgba(227, 237, 246, 1);
	--blue--2: rgba(218, 231, 243, 1);
	--blue--3: rgba(188, 206, 219, 1);
	--blue--4: rgba(157, 181, 195, 1);
	--blue--5: rgba(127, 156, 171, 1);
	--blue--6: rgba(99, 122, 138, 1);
	--blue--7: rgba(71, 87, 104, 1);
	--blue--8: rgba(43, 53, 71, 1);
	--blue--85: rgba(26, 32, 43, 1);
	--blue--9: rgba(17, 21, 28, 1);
	--blue--95: rgba(13, 16, 21, 1);
	--blue--10: rgba(9, 11, 14, 1);
	
	--green--0: rgba(234, 255, 243, 1);
	--green--05: rgba(230, 249, 222, 1);
	--green--1: rgba(225, 243, 200, 1);
	--green--15: rgba(177, 233, 182, 1);
	--green--2: rgba(129, 223, 164, 1);
	--green--3: rgba(94, 218, 149, 1);
	--green--4: rgba(47, 205, 118, 1);
	--green--5: rgba(0, 193, 87, 1);
	--green--6: rgba(0, 154, 70, 1);
	--green--7: rgba(0, 116, 52, 1);
	--green--8: rgba(0, 77, 35, 1);
	--green--85: rgba(0, 58, 26, 1);
	--green--9: rgba(0, 39, 17, 1);
	--green--95: rgba(0, 19, 9, 1);
	--green--10: rgba(0, 0, 0, 1);
	
	--red--0: rgba(255, 255, 255, 1);
	--red--05: rgba(253, 235, 236, 1);
	--red--1: rgba(250, 215, 217, 1);
	--red--15: rgba(248, 196, 198, 1);
	--red--2: rgba(245, 176, 179, 1);
	--red--3: rgba(245, 176, 179, 1);
	--red--4: rgba(236, 97, 102, 1);
	--red--5: rgba(231, 57, 64, 1);
	--red--6: rgba(185, 46, 51, 1);
	--red--7: rgba(139, 34, 38, 1);
	--red--8: rgba(92, 23, 26, 1);
	--red--85: rgba(69, 17, 19, 1);
	--red--9: rgba(46, 11, 13, 1);
	--red--95: rgba(23, 6, 6, 1);
	--red--10: rgba(0, 0, 0, 1);
	
	--yellow--0: rgba(255, 255, 255, 1);
	--yellow--05: rgba(255, 249, 230, 1);
	--yellow--1: rgba(255, 243, 205, 1);
	--yellow--15: rgba(255, 236, 181, 1);
	--yellow--2: rgba(255, 230, 156, 1);
	--yellow--3: rgba(255, 218, 106, 1);
	--yellow--4: rgba(255, 205, 57, 1);
	--yellow--5: rgba(255, 193, 7, 1);
	--yellow--6: rgba(204, 154, 6, 1);
	--yellow--7: rgba(153, 116, 4, 1);
	--yellow--8: rgba(102, 77, 3, 1);
	--yellow--85: rgba(77, 58, 2, 1);
	--yellow--9: rgba(51, 39, 1, 1);
	--yellow--95: rgba(26, 19, 1, 1);
	--yellow--10: rgba(0, 0, 0, 1);
	
	--gray--0: rgba(255, 255, 255, 1);
	--gray--05: rgba(247, 248, 248, 1);
	--gray--1: rgba(239, 240, 242, 1);
	--gray--15: rgba(230, 233, 235, 1);
	--gray--2: rgba(222, 225, 229, 1);
	--gray--3: rgba(206, 211, 215, 1);
	--gray--4: rgba(189, 196, 202, 1);
	--gray--5: rgba(173, 181, 189, 1);
	--gray--6: rgba(138, 145, 151, 1);
	--gray--7: rgba(104, 109, 113, 1);
	--gray--8: rgba(69, 72, 76, 1);
	--gray--85: rgba(52, 54, 57, 1);
	--gray--9: rgba(35, 36, 38, 1);
	--gray--95: rgba(17, 18, 19, 1);
	--gray--10: rgba(0, 0, 0, 1);
	
	/* Brand color style */
	--brand--primary: var(--green--5);
	--brand--secondary: var(--blue--5);
	--brand--accent: var(--red--5);
	
	/* Btn color style */
	--btn--active: var(--green--5);
	--btn--danger: var(--red--5);
	--btn--warning: var(--yellow--5);
	--btn--disabled: var(--gray--05);
	--btn--danger: var(--red--5);
	
	/* Badge color style */
	--bge--default: var(--blue--05);
	--bge--active: var(--green--1);
	--bge--danger: var(--red--1);
	--bge--warning: var(--yellow--15);
	--bge--warning: var(--yellow--15);
	--bge--info: var(--blue--2);
	
	/* Border color style */
	--border--primary: var(--gray--2);
	--border--secondary: var(--blue--1);
	--border--accent: var(--blue--8);
	--border--focus: var(--green--5);
	
	/* Text color style */
	--text--primary: var(--blue--8);
	--text--secondary: var(--blue--5);
	--text--tertiary: var(--blue--2);
	--text--active: var(--green--5);
	--text--highlight: var(--green--3);
	--text--placeholder: var(--gray--3);
	--text--white: var(--gray--0);
	
	/* Text styles */
	--logo--size: 1.5rem; /* 34px */
	--logo--weight: 800; 
	--page--title--size: 1.6rem; /* 28px */
	--page--title--weight: 700;
	--container--title--size: 1.1rem; /* 18px */
	--container--title--weight: 700;
	--primary--size: 1rem; /* 17px */
	--primary--weight: 400;
	--secondary--size: 0.9rem; /* 16px */
	--secondary--weight: 300;
	--accent--size: 1.1rem; /* 18px */
	--accent--weight: 700;
	
	/* Font-family */
	--brand-font : 'NanumSquare', sans-serif;
	
	/* Border-radius style */
	--radius--s: 2px; /* btn, input */
	--radius--m: 5px;
	--radius--l: 10px; /* backgroud */
	
	/* Padding style */
	--pd--s: 10px;
	--pd--m: 20px;
	--pd--l: 30px;
	--pd--xl: 40px;
	
	/* Gap style */
	--gap--s: 10px;
	--gap--m: 20px;
	--gap--l: 30px;
	--gap--xl: 40px;
}

/* layout style */
.row,
.container-fluid {
	margin: 0 !important;
}
.content-wrapper {
	background-color: var(--blue--05);
/* 	min-height: 1525px !important; */
}
.content-header {
	padding: var(--pd--s) 0 var(--pd--m) 0
}
.content-wrapper .content{
	padding: var(--pd--s) 30px 50px 25px
}
.wrap {
	display: flex;
	flex-direction: column;
	gap: var(--gap--m);
	min-width: 700px;
}
.cont {
	padding: var(--pd--m);
	border-radius: var(--radius--m);
	background-color: var(--gray--0);
	display: flex;
	flex-direction: column;
	gap: var(--gap--m);
	border: 1px solid var(--gray--15);
}
.chart-wrap {
    display: flex;
    gap: 20px;
    justify-content: space-between;
}
.cont.chart {
    width: 60%;
}
.cont.table {
    width: 40%;
    margin: 0;
}
.cont-title {
	font-size: var(--accent--size);
	font-weight: var(--container--title--weight);
}

.crow {
	margin: 0 !important;
	gap : 20px;
	display: flex;
}

/* 공통 스타일 */
body {
	font-family: var(--brand-font);
}
h1 {
	font-size: var(--page--title--size) !important;
	font-weight: var(--page--title--weight);
}

/* 인풋 디자인 */
input {
	border: 1px solid var(--border--primary);
	height: 44px;
	padding: 10px;
	border-radius: var(--radius--m);
	font-size: var(--primary--size);
	background-color: var(--gray--0);
}
input:disabled {
	background-color: var(--gray--1);
}
input[type="date"] {
	-webkit-user-select: none !important; 
	-moz-user-select: -moz-none !important;
	-ms-user-select: none !important;
	user-select: none !important;
}
.check-btn {
    display: none;
}
.check-btn + label {
    cursor: pointer;
    user-select: none;
}
.check-btn + label > span {
    vertical-align: middle;
    padding-left: 5px;
    font-weight: 400;
}
.check-btn + label:before { /* label:before에 체크하기 전 상태 CSS */
    content: "";
    display: inline-block;
    width: 20px;
    height: 20px;
    border: 2px solid var(--gray--5);
    background-image: url('/resources/images/common/check-gray.png');
    border-radius: 4px;
    vertical-align: middle;
}
.check-btn:checked + label:before { /* label:before에 체크된 상태 CSS */
    background-color: var(--green--5);
    border-color: var(--green--5);
    background-image: url('/resources/images/common/check.png');
    background-repeat: no-repeat;
    background-size: 20px;
    background-position: 50%;
}
