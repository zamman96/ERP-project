/* 셀렉트 디자인 */
select,
.select2-container--default .select2-selection--single { /*화살표 기본 css 없애기*/
  	border: 1px solid var(--border--primary) !important;
	height: 44px !important;
	padding: 10px;
	border-radius: var(--radius--m);
	font-size: var(--primary--size);
	background-color: var(--gray--0) !important;
	
	/* 화살표 커스텀 */
	-o-appearance: none;
	-webkit-appearance: none;
	-moz-appearance: none;
	appearance: none;
	background: url(/resources/images/common/keyboard_arrow_down.png) no-repeat 97% 50%;
    background-size: 20px 20px;
	
	/* 말줄임 처리 */
	width: 99%;
	white-space: nowrap;
	text-overflow: ellipsis;
	overflow: hidden !important;
}
select option {
  background: var(--gray--0);
  color: var(--gray--0);
  padding: 3px 0;
}

/* select2 라이브러리 */
.select2-selection--single {
	border-radius: 4px !important;
}
.select2-selection--single:hover {
	border: 1px solid var(--border--focus) !important;
}
.select2-container--default .select2-selection--single .select2-selection__arrow {
	display: none;	
}
.select2-container--default .select2-search--dropdown .select2-search__field  {
	height: 38px;
}
.select2-container .select2-selection--single .select2-selection__rendered {
    padding-left: 0 !important;
    padding-right: 0 !important;
}
.select2-container--default .select2-results__option {
    padding: var(--pd--s) 12px !important;
}
.select2-container--default .select2-results__option[aria-selected=true] {
    background-color: var(--gray--1) !important;
    color: var(--text--primary);
}
.select2-container--default .select2-results__option:hover,
.select2-results__option--highlighted {
    background: var(--green--5) !important;
}
.select2-container--default .select2-results>.select2-results__options {
    max-height: 300px !important;
}
.select2-container--default .select2-dropdown {
    border-top: 1px solid var(--border--primary) !important;
    border: 1px solid var(--border--primary);
    top: 5px;
}

/* select 커스텀 */
.select-custom {
  position: relative;
  height: 44px;
}
.select-custom select {
  display: none; /*hide original SELECT element:*/
}
.select-custom select:disabled + .select-selected {
    background-color: var(--gray--1) !important;
    pointer-events: none;
}
.select-selected {
  	border: 1px solid var(--border--primary) !important;
	height: 44px !important;
	padding: 0 10px !important;
	border-radius: var(--radius--m);
	font-size: var(--primary--size);
	background-color: var(--gray--0) !important;
	line-height: 2.5;
	
	/* 화살표 커스텀 */
	-o-appearance: none;
	-webkit-appearance: none;
	-moz-appearance: none;
	appearance: none;
	background: url(/resources/images/common/keyboard_arrow_down.png) no-repeat 97% 50%;
    background-size: 20px 20px;
	
	/* 말줄임 처리 */
	width: 100%;
	white-space: nowrap;
	text-overflow: ellipsis;
	overflow: hidden !important;
}
.select-selected:hover {
	border: 1px solid var(--border--focus) !important;
}
/*point the arrow upwards when the select box is open (active):*/
.select-selected.select-arrow-active:after {
  border-color: transparent transparent #fff transparent;
  top: 7px;
}
/*style the items (options), including the selected item:*/
.select-items div,.select-selected {
	color: var(--text--primary);
	cursor: pointer;
	user-select: none;
}
.select-items div {
	padding: var(--pd--s) 12px !important;
}
/*style items (options):*/
.select-items {
	position: absolute;
    background-color: var(--gray--0);
    top: 110%;
    left: 0;
    right: 0;
    z-index: 999;
    border: 1px solid var(--border--primary);
    border-radius: 4px !important;
}
/*hide the items when the select box is closed:*/
.select-hide {
  display: none;
}
.same-as-selected {
	background-color: var(--gray--1);
	color: var(--text--primary) !important;
}
.select-items div:hover {
  background-color: var(--green--5) !important;
   color: var(--gray--0) !important;
}
.select2-dropdown {
    top: 2px;
    border-radius: 4px !important;
}
.select2-container .select2-selection--single .select2-selection__rendered {
    padding-left: 0 !important;
    padding-right: 0 !important;
}

/* 버튼 스타일 */
button{
	padding: 10px 15px;
	border-radius: var(--radius--m);
	font-size: var(--primary--size);
	border: none;
}
button:focus-visible {
	outline: none;
}
button.btn-default {
	border: 1px solid var(--border--primary);
	color: var(--text--secondary);
	background-color: var(--gray--0);
}
button.btn-default:hover{
	background-color: var(--gray--05);
}
button.btn-active {
	background-color: var(--btn--active);
	color: var(--text--white);
}
button.btn-active:hover {
	background-color: var(--green--4);
}
button.btn-disabled {
	background-color: var(--btn--disabled);
	color: var(--text--placeholder);
	padding: 5px 10px;		
}
button.btn-disabled:hover {
	background-color: var(--btn--disabled);
	color: var(--text--placeholder);
	cursor: not-allowed !important;
}
.btn-default:disabled {
    color: var(--gray--3);
    background-color: #f8f9fa;
}
button.btn-default.search {
	color: var(--gray--0);
    background-color: var(--blue--8);
}
button.btn-danger {
	background-color: var(--btn--danger);
	color: var(--text--white);
}
.btn-danger:focus {
	background-color: var(--btn--danger);
	color: var(--text--white);
}
button.btn-danger:hover {
	background-color: var(--red--4);
}
button.btn-default.search .icon {
	font-size: 1.2rem !important;
	color: var(--gray--0);
}
button.btn-download {
   	border: 1px solid var(--border--primary);
	color: var(--text--secondary);
	background-color: var(--gray--0);
    padding: 5px 10px;
}
button.btn-download:hover {
    color: var(--text--secondary);
}

/* 뱃지 스타일 */
.bge {
	font-size: var(--secondary--size);
	font-weight: var(--secondary--weight);
	padding: 4px 10px;
	border-radius: 50px;
	border: none;
}
.bge-default {
	background-color: var(--bge--default);
}
.bge-active {
	background-color: var(--bge--active);
}
.bge-danger {
	background-color: var(--bge--danger);
}
.bge-warning {
	background-color: var(--bge--warning);
}
.bge-info {
	background-color: var(--bge--info);
}
.bge-default-border {
	border: 1px solid var(--blue--3);
    color: var(--blue--5);
    font-weight: bold;
}
.bge-active-border {
	border: 1px solid var(--green--6);
    color: var(--green--6);
    font-weight: bold;
}
.bge-danger-border {
	border: 1px solid var(--red--6);
    color: var(--red--6);
    font-weight: bold;
}
.bge-warning-border {
	border: 1px solid var(--btn--warning);
    color: var(--btn--warning);
    font-weight: bold;
}
.bge-info-border {
	border: 1px solid var(--blue--7);
    color: var(--blue--7);
    font-weight: bold;
}

/* 검색 영역 */
.search-wrap{
	display: flex;
	flex-direction: row;
    flex-wrap: wrap;
    gap: var(--gap--m);
}
.search-cont {
	display: flex;
	flex-direction: column;
	gap: var(--gap--s);
}
.search-title {
	font-size: 0.8rem;
}
.search-input-btn {
	display: flex;
	position: relative;
}
.search-input{
	width: 300px;
}
.search-btn {
	border: none;
	background: var(--gray-0);
	background: url(/resources/images/common/search.png) no-repeat 50% 50%;
    background-size: 20px 20px;
	padding: 15px;
    position: absolute;
    top: 15%;
    right: 10px;
}
.search-word-wrap { /* 검색어 셀렉트 박스 */
	display: flex;
	gap: 4px;
}
.search-date-wrap { /* 일자 셀렉트 박스 */
/* 	display: flex;
	gap: 4px; */
}

/* 검색어 영역 스타일 */
.search-control {
    display: flex;
    justify-content: flex-end;
    padding: 8px var(--pd--m);
    /* background-color: #eaeef5; */
    background-color: #f2f2f2;
    border-radius: 0 0 5px 5px;
    border: 1px solid #dcdfe2;
    margin-top: -3px;	
}
.search-control-btns {
    display: flex;
    align-items: center;
    gap: 15px;
}
.search-reset {
	cursor: pointer;
    gap: 4px;
}
.search-reset:hover {
	font-weight: bold;
}
.search-reset .icon {
    background-color: var(--text--primary);
    color: var(--gray--0);
    border-radius: 5px;
    font-size: 18px !important;
    margin-left: 4px;	
}
.search-toggle {
	cursor: pointer;
    gap: 4px;
}
.search-toggle:hover {
	font-weight: bold;
}
.search-toggle .icon {
    background-color: var(--text--primary);
    color: var(--gray--0);
    border-radius: 5px;
    font-size: 18px !important;
    margin-left: 4px;
}

/* 검색 접기 영역 스타일 */
.search-summary {
	display: none;
}
.summary {
	margin-left: 5px;
    font-weight: bold;
    font-size: 14px;
}
.divide-border {
	border-right: 1px solid var(--text--primary);
}

/* 모달창 */
.modal-header {
	padding: var(--pd--m) !important;
    background: var(--blue--8);
}
.modal-title {
	color: var(--gray--0); !important;
	font-size: 20px;
	font-weight: bold !important;
	line-height: 2 !important;
}
.swal2-actions {
    gap: var(--gap--s);	
}

/* 아이콘 */
.material-symbols-outlined {
	color: var(--text--secondary);
	font-variation-settings:
	'FILL' 0,
	'wght' 400,
	'GRAD' 0,
	'opsz' 20
}
.icon {
    font-size: 16px !important;
	vertical-align: middle; 
}
.btn-active .icon {
	color: var(--gray--0)
}
.fix-icon {
	color: var(--brand--accent);
	font-size: 1.3rem !important;
	font-variation-settings:
	'FILL' 1,
	'wght' 300,
	'GRAD' 0,
	'opsz' 20
