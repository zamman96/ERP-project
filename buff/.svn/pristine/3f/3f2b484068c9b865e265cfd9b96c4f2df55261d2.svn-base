@charset "UTF-8";

#salesChart{
	display: block;
    box-sizing: border-box;
    height: 500px !important;
    width: 706px !important;
    position: relative;
    bottom: -28px;
}

.page-tap-wrap{
	position : relative;
	top : 20px;
}

#month-text{
	position: relative;
